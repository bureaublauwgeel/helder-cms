{
    "name": "bureaublauwgeel/helder-cms",
    "license": "MIT",
    "type": "project",
    "description": "The \"Helder CMS\" from Bureaublauwgeel",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=7.0.22",
        "kunstmaan/bundles-cms": "~3.6.0",
        "friendsofsymfony/http-cache-bundle": "~1.3",
        "doctrine/doctrine-migrations-bundle": "~1.3",
        "friendsofsymfony/user-bundle": "2.0.0-beta1",
        "twig/twig": "~1.35",
        "bureaublauwgeel/kunstmaanadminbundle": "3.6.0.*",
        "bureaublauwgeel/generatorbundle": "~1.0",
        "bureaublauwgeel/base-bundle": "~1.0",
        "bureaublauwgeel/PagePartBundle": "~1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@git.bbgtest.nl:bureaublauwgeel/bbg-kunstmaan-admin-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@git.bbgtest.nl:bureaublauwgeel/bbg-generator-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@git.bbgtest.nl:bureaublauwgeel/bbg-base-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@git.bbgtest.nl:bureaublauwgeel/bbg-page-part-bundle.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.0.22"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
